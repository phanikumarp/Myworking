alias alllog='ps -ef | grep -v grep| grep all_log'
alias errorlog='ps -ef | grep -v grep| grep error_log.jar'
alias lsid='ps -ef | grep -v grep| grep logstash'
alias lsrestart='service logstash restart'
alias lsstatus='service logstash status'
alias lsstop='service logstash stop'
alias redstatus='service redis_6379 status'
alias redid='ps -ef | grep -v grep| grep redis'
alias esid='ps -ef | grep -v grep| grep elastic'
alias esstart='/home/elasticsearch-1.4.4/bin/elasticsearch -d -Xmx2g -Xms2g'
